{
  "expo": {
    "name": "Antonio Melli App",
    "slug": "Antonio Melli App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/favicon.jpg",
    "scheme": "Antonio Melli App",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.saikmenage.siteapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/favicon.jpg",
        "backgroundColor": "#0c1634"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.saikmenage.siteapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e55cab98-a89e-4056-a3bf-9aa566d83d0b"
      }
    }
  }
}
