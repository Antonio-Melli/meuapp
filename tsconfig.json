{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-jsx", // ou "react" dependendo da vers√£o do React
    "module": "esnext",
    "target": "es6",
    "lib": ["dom", "esnext"],
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
